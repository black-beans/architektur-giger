<h2>Architektur</h2>
<p>Planung und Ausführung von Hochbauten: Neubau, Umbau und Renovation.</p>

<table class="architecture">
  <% current_page.data.galleries.each_with_index do |area, area_index| %>
    <% if area.galleries %>
      <% area.galleries.each_with_index do |gallery, gallery_index| %>
        <tr class="<%= gallery_index == 0 ? 'border' : '' %>">
          <td><%= gallery_index == 0 ? area.name : '' %></td>
          <td>
            <% if gallery.galleries %>
              <% gallery.galleries.each_with_index do |subgallery, subgallery_index| %>
                <% subgallery.photos.each_with_index do |photo, photo_index| %>
                  <a class="gallery <%= photo_index == 0 ? 'first' : 'other' %>" href="/<%= photo.path %>" rel="group-<%= area_index %>-<%= gallery_index %>-<%= subgallery_index %>" title="<%= photo.title %>"><%= subgallery.name %></a>
                  <% if photo_index == 0 %><br><% end %>
                <% end %>
              <% end %>
            <% else %>
              <% if gallery.photos %>
                <% gallery.photos.each_with_index do |photo, photo_index| %>
                  <a class="gallery <%= photo_index == 0 ? 'first' : 'other' %>" href="/<%= photo.path %>" rel="group-<%= area_index %>-<%= gallery_index %>" title="<%= photo.title %>">Fotoalbum</a>
                  <% if photo_index == 0 %><br><% end %>
                <% end %>
              <% end %>
            <% end %>
          </td>
          <td>
            <h3><%= gallery.name %></h3>
            <p><%= gallery.description %></p>
            <% if gallery.documents %>
              <ul class="docs">
                <% gallery.documents.each do |document| %>
                  <li><a href="<%= document.path %>"><%= document.title %></a></li>
                <% end %>
              </ul>
            <% end %>
          </td>
        </tr>
      <% end %>
    <% else %>
      <% if area.photos %>
        <tr class="border">
          <td><%= area.name %></td>
          <td>
            <% area.photos.each_with_index do |photo, photo_index| %>
              <a class="gallery <%= photo_index == 0 ? 'first' : 'other' %>" href="/<%= photo.path %>" rel="group-<%= area_index %>" title="<%= photo.title %>">Fotoalbum</a>
              <% if photo_index == 0 %><br><% end %>
            <% end %>
          </td>
          <td>
            <p><%= area.description %></p>
          </td>
        </tr>
      <% end %>
    <% end %>
  <% end %>
</table>
